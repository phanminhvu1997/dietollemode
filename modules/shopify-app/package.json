    "lodash": "4.17.21",
    "moment": "2.29.1",
    "mongoose": "5.13.11",
    "morgan": "1.10.0",
    "ms": "2.1.2",
    "node-fetch": "2.6.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-highlight-words": "0.17.0",
