    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-middleware": "3.7.2",
    "webpack-s3-plugin": "1.0.3"
  },
  "scripts": {
    "lint:fix": "eslint --ext js --ext jsx --fix --ext .yaml ./src",
    "start": "babel-node src/server/index.js",
    "proxy": "ngrok http $(grep PORT .env | cut -d '=' -f2) -authtoken=$(grep NGROK_AUTHTOKEN .env | cut -d '=' -f2)",
    "dev-server": "run.env nodemon --exec babel-node .dev/dev-server.js --watch .dev --ignore **/*.json",
    "server": "run.env nodemon --exec babel-node src/server/index.js --watch src/server --ignore **/*.json",
    "dev": "concurrently --raw \"npm run lint:fix\" \"npm run dev-server\" \"npm run server\"",
    "webpack": "run.env webpack --config .dev/webpack.babel.js"
  }
}
